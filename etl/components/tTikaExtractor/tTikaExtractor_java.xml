<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="BETA"
    COMPATIBILITY="ALL"
    AUTHOR="fx.prunayre@gmail.com"
    RELEASE_DATE="20120124"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="1"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.pdf"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="WRITE_LIMIT" FIELD="TEXT" NUM_ROW="2" REQUIRED="true"
      REPOSITORY_VALUE="WRITE_LIMIT">
      <DEFAULT>"30000"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="tika"
        MODULE="tika-app-1.0.jar"
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="METADATA_OBJ" TYPE="id_Object" AVAILABILITY="FLOW"/>
    <RETURN NAME="METADATA" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="CONTENT" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="CONTENT_XHTML" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="PARSE_ERROR" TYPE="id_String" AVAILABILITY="FLOW"/>
    <RETURN NAME="ERROR" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>
